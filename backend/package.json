{
  "name": "geoguessr-stats",
  "version": "1.0.0",
  "description": "Web application for analyzing GeoGuessr game statistics",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=models",
    "test:api": "jest --testPathPattern=api",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "geoguessr",
    "statistics",
    "analytics",
    "typescript",
    "mongodb"
  ],
  "author": "Colin",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.6.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "jest-extended": "^6.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "mongodb-memory-server": "^10.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.1"
  }
}
